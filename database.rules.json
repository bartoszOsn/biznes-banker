{
	"rules": {
		"sessions": {
			"$sessionId": {
				".read": true,
				".write": "(!data.exists()) || auth != null",
				"users": {
					".write": false,
					"$uid": {
						".write": "auth != null && $uid === auth.uid"
					}
				},
				"bankerUserId": {
					".write": "data.val() == null || (auth != null && auth.uid === data.val())",
					".validate": "newData.val() == null || root.child('sessions/' + $sessionId + '/users/' + newData.val()).exists()"
				},
				"$other": {
					".write": "auth != null && root.child('sessions/' + $sessionId + '/users/' + auth.uid).exists()"
				}
			}
		}
	}
}
